{
  "name": "WiredPanels",
  "version": "0.0.0-semantic-release",
  "authors": [
    "Alexander MeiÃŸner",
    "Markus Felten"
  ],
  "license": "MIT",
  "description": "Panels with sockets connected by bezier curve wires",
  "module": "WiredPanels.js",
  "scripts": {
    "xtest": "node test/server.js",
    "semantic-release": "semantic-release",
    "cover": "nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md"
  },
  "bugs": {
    "url": "https://github.com/Symatem/WiredPanels/issues"
  },
  "homepage": "https://github.com/Symatem/WiredPanels#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Symatem/WiredPanels.git"
  },
  "dependencies": {},
  "devDependencies": {
    "semantic-release": "^9.1.0",
    "jsdoc-to-markdown": "^3.0.0",
    "jsdoc-babel": "^0.3.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.3.0",
    "ava": "^0.23.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-multi-entry": "^2.0.1",
    "xo": "^0.19.0"
  },
  "engines": {
    "node": ">=8.8.1"
  },
  "main": "dist/index.js",
  "keywords": [
    "npm-package-template"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
